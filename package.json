{
   "name": "gulp-node-slate",
   "description": "node-slate as a gulp task",
   "version": "2.0.2",
   "license": "MIT",
   "type": "module",
   "main": "gulp-node-slate.js",
   "repository": "github:center-key/gulp-node-slate",
   "homepage": "https://github.com/center-key/gulp-node-slate",
   "bugs": "https://github.com/center-key/gulp-node-slate/issues",
   "docs": "https://github.com/center-key/gulp-node-slate#readme",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "api-documentation",
      "javascript",
      "node-slate",
      "nodejs",
      "gulp",
      "gulp-plugin",
      "slate",
      "static-site-generator"
   ],
   "jshintConfig": {
      "esversion": 11,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "node": true,
      "mocha": true
   },
   "scripts": {
      "pretest": "jshint . --exclude-path .gitignore",
      "test": "mocha spec.js",
      "posttest": "html-validator api-docs"
   },
   "dependencies": {
      "chalk": "~5.2",
      "child_process": "~1.0",
      "node-slate": "~3.1",
      "path": "~0.12",
      "plugin-error": "~2.0",
      "through2": "~4.0"
   },
   "devDependencies": {
      "assert-deep-strict-equal": "~1.0",
      "gulp": "~4.0",
      "jshint": "~2.13",
      "mocha": "~10.2",
      "string-to-stream": "~3.0",
      "w3c-html-validator": "~1.3"
   }
}
